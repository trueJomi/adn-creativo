---
import Footer from "./Footer.astro";
import LayoutInit from "./LayoutInit.astro";
import NavBar from "./NavBar.astro";

interface Props {
    class?: string;
}

const { class: className }: Props = Astro.props;
---

<LayoutInit>
    <NavBar/>
    <main class:list={[
        "bg-background p-5 space-y-3",
        className
    ]} >
        <slot/>
    </main>
    <Footer/>
</LayoutInit>